    "create-installer-win": "tsc main.ts && ng build --prod --aot --base-href ./ && electron . && node installers/windows/createinstaller.js",
    "package-linux": "tsc main.ts && ng build --prod --aot --base-href ./ && electron . && electron-packager . xcash-desktop-wallet --overwrite --ignore=node_modules --platform=linux --arch=x64 --out=release-builds",
    "create-installer-linux": ""
