    "e2e": "ng e2e",
    "package-mac": "tsc main.ts && ng build --prod --aot --base-href ./ && electron . && electron-packager . xcash-desktop-wallet --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
